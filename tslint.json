{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single"],
    "space-before-function-paren": [true, "always"],
    "semicolon": [true, "never"],
    "no-console": false,
    "eofline": false,
    "no-consecutive-blank-lines": true,
    "object-literal-sort-keys": false,
    "trailing-comma": false
  }
}